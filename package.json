{
  "name": "the-deep-interview",
  "version": "1.0.0",
  "description": "Comprehensive AI-powered interview simulator with voice interaction",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon --config nodemon.json server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:server": "nodemon --config nodemon.json server/index.js",
    "dev:stable": "node start-server.js",
    "run": "node run-app.js",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "interview",
    "ai",
    "voice",
    "resume",
    "deep learning"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.0",
    "pdf-parse": "^1.1.1",
    "socket.io": "^4.7.2",
    "uuid": "^11.1.0"
  }
}
